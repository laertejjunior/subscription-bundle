{
    "name": "laertejjunior/subscription-bundle",
    "description": "Symfony Bundle for manage user subscriptions",
    "keywords": [
        "Subscription",
        "Subscription Management",
        "Plans",
        "Products"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/laertejjunior/subscription-bundle",
    "support": {
        "email": "support@shapeocode.de",
        "issues": "https://github.com/laertejjunior/subscription-bundle/issues",
        "source": "https://github.com/laertejjunior/subscription-bundle/releases",
        "wiki": "https://github.com/laertejjunior/subscription-bundle/wiki"
    },
    "authors": [
        {
            "name": "David PÃ©rez Terol",
            "email": "davidtxnet@gmail.com"
        },
        {
            "name": "Nikita Loges",
            "homepage": "https://loges.one",
            "email": "dev@loges.one"
        }
    ],
    "require": {
        "php": ">=8.0.2",

        "symfony/yaml": "6.0.*",
        "symfony/framework-bundle": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/event-dispatcher": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/monolog-bundle": "~3.1",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "doctrine/common": "^3.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\SubscriptionBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\SubscriptionBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
