{
    "name": "shapecode/subscription-bundle",
    "description": "Symfony Bundle for manage user subscriptions",
    "keywords": [
        "Subscription",
        "Subscription Management",
        "Plans",
        "Products"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/subscription-bundle",
    "support": {
        "email": "support@shapeocode.de",
        "issues": "https://github.com/shapecode/subscription-bundle/issues",
        "source": "https://github.com/shapecode/subscription-bundle/releases",
        "wiki": "https://github.com/shapecode/subscription-bundle/wiki"
    },
    "authors": [
        {
            "name": "David PÃ©rez Terol",
            "email": "davidtxnet@gmail.com"
        },
        {
            "name": "Nikita Loges",
            "homepage": "https://loges.one",
            "email": "dev@loges.one"
        }
    ],
    "require": {
        "php": "~7.2",

        "symfony/yaml": "~3.4|~4.0",
        "symfony/framework-bundle": "~3.4|~4.0",
        "symfony/console": "~3.4|~4.0",
        "symfony/event-dispatcher": "~3.4|~4.0",
        "symfony/security": "~3.4|~4.0",
        "symfony/monolog-bundle": "~3.1",

        "doctrine/orm": "~2.6",
        "doctrine/common": "~2.8",
        "doctrine/doctrine-bundle": "~1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2",
        "mockery/mockery": "~0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\SubscriptionBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\SubscriptionBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
